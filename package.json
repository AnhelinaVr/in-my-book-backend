    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^18.11.19",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "nock": "^13.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  }
